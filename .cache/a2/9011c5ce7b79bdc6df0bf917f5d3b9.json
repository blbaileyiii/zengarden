act;\r\n    top: $console-p-row;\r\n    left: $console-p-column;\r\n    &:hover {\r\n      @include expand;\r\n    }\r\n  }\r\n\r\n  .participation {\r\n    @include contract;\r\n    bottom: $console-p-row;\r\n    left: $console-p-column;\r\n    &:hover {\r\n      @include expand;\r\n    }\r\n  }\r\n\r\n  .benefits {\r\n    @include contract;\r\n    bottom: $console-p-row;\r\n    right: $console-p-column;\r\n    &:hover {\r\n      @include expand;\r\n    }\r\n  }  \r\n\r\n  .requirements {\r\n    @include contract;\r\n    top: $console-p-row;\r\n    right: $console-p-column;\r\n    &:hover {\r\n      @include expand;\r\n    }\r\n  }\r\n\r\n  .wrapper {\r\n    display: block;\r\n  }\r\n\r\n  footer {\r\n    position: fixed;\r\n    bottom: 0;\r\n    left: 50vw;\r\n    transform: translate(-50%, 0);\r\n  }\r\n\r\n  .design-selection {\r\n    border: none;\r\n  }\r\n\r\n  @keyframes expand-console {\r\n    0% {\r\n      width: $console-w-narrow;\r\n      max-height: $console-maxh-narrow;\r\n    }\r\n    50% {\r\n      width: $console-w-expanded;\r\n      max-height: $console-maxh-narrow;\r\n    }\r\n    100% {\r\n      width: $console-w-expanded;      \r\n      max-height: $console-maxh-expanded;\r\n    }\r\n  }\r\n\r\n  @keyframes contract-console {\r\n    100% {\r\n      width: $console-w-narrow;\r\n      max-height: $console-maxh-narrow;\r\n    }\r\n    50% {\r\n      width: $console-w-expanded;\r\n      max-height: $console-maxh-narrow;\r\n    }\r\n    0% {\r\n      width: $console-w-expanded;      \r\n      max-height: $console-maxh-expanded;\r\n    }\r\n  }\r\n}","scss\\partials\\_var.scss":"// VARIABLES:\r\n\r\n// breakpoints:\r\n\r\n$M: 40rem;\r\n$L: 70rem;\r\n\r\n// dimensions:\r\n$console-w-narrow: 17vw;\r\n$console-w-expanded: 58vw;\r\n$border-width: 1px;\r\n$console-maxh-narrow: 3.5rem;\r\n$console-maxh-expanded: 50rem;\r\n\r\n// positions:\r\n$side-bar-top: 0;\r\n$console-p-column: 20vw;\r\n$console-m-column: 50vw;\r\n$console-p-row: 20vh;\r\n$console-m-row